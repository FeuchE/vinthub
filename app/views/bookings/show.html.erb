<div class="container booking-details">
  <h1>Booking Details</h1>

  <%= render 'show_booking_card',
    title: @booking.item.title,
    booking: @booking,
    price: @booking.item.price,
    photo: @booking.item.photo.key,
    start_date: @booking.start_date,
    end_date: @booking.end_date
  %>

  <%= link_to 'Back to Bookings', :back, class: 'button-link' %>
</div>
